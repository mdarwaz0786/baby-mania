{
  "name": "baby",
  "version": "1.0.0",
  "description": "e-commerce app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "frontend": "npm run dev --prefix ./frontend",
    "admin": "npm run dev --prefix ./admin",
    "backend": "nodemon ./backend/server.js",
    "server": "node ./backend/server.js",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\" \"npm run admin\"",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend && npm install --prefix admin && npm run build --prefix admin"
  },
  "keywords": [
    "e-commerce",
    "app"
  ],
  "author": "MdArbaz",
  "license": "ISC",
  "dependencies": {
    "cloudinary": "^2.2.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.4",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.0"
  }
}
